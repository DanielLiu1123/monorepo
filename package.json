{
  "name": "monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "Enterprise monorepo with multi-language microservices",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "clean": "turbo run clean && rm -rf node_modules",
    "proto": "make proto",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\""
  },
  "devDependencies": {
    "@turbo/gen": "^2.3.3",
    "prettier": "^3.6.2",
    "turbo": "^2.3.3",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=24.10.0",
    "pnpm": ">=10.20.0"
  },
  "packageManager": "pnpm@10.20.0"
}
